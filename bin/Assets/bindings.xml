<?xml version="1.0" encoding="utf-8"?>
<!-- Description des bindings de touches clavier -->
<!-- Codes:
fleches : UP, DOWN, LEFT, RIGHT
Lettres : toutes
Espace : SPACE
Echap : ESCAPE
-->
<bindings>

	<bind type="MOVE_LEFT">
		<key name="LEFT">X</key>
	</bind>
	
	<bind type="MOVE_RIGHT">
		<key name="RIGHT"></key>
	</bind>
	
	<bind type="JUMP">
		<key name="SPACE"></key>
	</bind>
	
	<bind type="ATTACK">
		<key name="V"></key>
	</bind>
	
	<bind type="QUIT">
		<key name="ESCAPE"></key>
	</bind>

</bindings>